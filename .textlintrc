{
  "plugins": [
    "jtf-style"
  ],
  "rules": {
    "spellcheck-tech-word": true,
    "no-mix-dearu-desumasu": true,
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "prh": {
      "rulePaths": [
        "prh.yml"
      ]
    },
    "jtf-style/2.1.2.漢字": false,
    "jtf-style/4.2.2.疑問符(？)": false,
    "jtf-style/4.2.7.コロン(：)": false,
    "jtf-style/2.1.6.カタカナの長音": false,
    "jtf-style/2.1.5.カタカナ": false,
    "jtf-style/3.1.1.全角文字と半角文字の間": false,
    "jtf-style/4.2.6.ハイフン(-)": false,
    "jtf-style/4.3.1.丸かっこ（）": false
  }
}